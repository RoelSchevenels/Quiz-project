<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml" fx:controller="javafFXpanels.ConnectToQuiz.ConnectToQuizController">
  <children>
    <VBox id="VBox" alignment="CENTER" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Text fx:id="quizText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="quiz-naam" text="" />
        <HBox id="HBox" alignment="CENTER" spacing="15.0">
          <children>
            <Button mnemonicParsing="false" onAction="#refresh" text="refresh" />
            <Button mnemonicParsing="false" onAction="#addToQuiz" text="Doe mee aan quiz" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
  <stylesheets>
    <URL value="@connecttoquiz.css" />
  </stylesheets>
</AnchorPane>
